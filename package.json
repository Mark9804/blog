    "markdown-it-mathjax3": "^4.3.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "typescript": "^5.6.3",
    "unocss": "0.64.1",
    "unplugin-auto-import": "^0.18.0",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vitepress": "1.5.0",
    "vitepress-sidebar": "^1.22.0",
    "vue": "3.5.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
