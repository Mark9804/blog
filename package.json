{
  "name": "vitepress-project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs --host",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "preview": "pnpm build && vitepress preview docs",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
