    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vueuse/components": "^11.2.0",
    "@vueuse/core": "^11.2.0",
    "cross-spawn": "^7.0.3",
    "dayjs": "^1.11.11",
    "eslint": "^9.0.0",
