    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@unocss/preset-icons": "0.64.1",
    "autoprefixer": "^10.4.19",
